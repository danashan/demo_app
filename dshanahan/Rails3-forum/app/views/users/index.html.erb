<h1>Users</h1>
<div class="listing">
<table>
  <tr>
	   <th>Username</th>
    <th>First</th>
    <th>Last</th>
 
    <th>Email</th>
    <th>Active</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
	  <td><%= link_to user.username , user%></td>
    <td><%= user.first_name %></td>
    <td><%= user.last_name %></td>
  
    <td><%= user.email %></td>
    <td><%= user.active %></td>
  

  </tr>
<% end %>
</table>

<br />
</div>
<%= link_to 'New User', new_user_path %>
